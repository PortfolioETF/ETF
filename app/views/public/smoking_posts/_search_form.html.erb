<div id="smoking-post-search-form">
<div id="smo-search">
<%= form_with url: smoking_search_path, html: {method: :get} do |f| %>
  <%= f.text_field :address_cont, placeholder: "住所で検索", class: "search-form"%>
  <%= f.submit '検索',onclick: "searchLocation()", type: "button", class: "search-button"%>
<% end %>
</div>
</div>
